<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9E6C3"
    tools:context=".FavorilerSayfasi">


    <ImageButton
        android:id="@+id/profilbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="100dp"
        android:layout_marginBottom="40dp"
        android:background="@null"
        app:srcCompat="@drawable/ic_person"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/tarifbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="290dp"
        android:layout_marginBottom="40dp"
        android:background="@null"
        app:srcCompat="@drawable/ic_food"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/profilimyasizi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="20dp"
        android:text="Profilim"
        android:textStyle="bold" />



    <TextView
        android:id="@+id/tarifleryasizi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="20dp"
        android:text="Tarifler"
        android:textStyle="bold" />

</RelativeLayout>